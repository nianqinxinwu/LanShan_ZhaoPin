.container {
  background-color: #f5f5f5;
  min-height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}


.container .header{
  width:750rpx;
  height:300rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  
} 

.container .header .pic{

width:750rpx;
height: 250rpx;
align-self: center;
display: flex;
flex-direction: row;
align-items:center;
padding-left:20rpx;

}
.container .header .pic image{
   width:60rpx;
   height: 60rpx;
   border:0rpx solid #FFF; 
   border-radius: 60rpx;
 
}
.container .header .pic .userinfo{

   width:580rpx;
    font-size: .8rem;
  margin-left: 15rpx;
   color: #FFF;
  display: flex;
  flex-direction: column;
  justify-content: space-between;


}



  .noteoper{

