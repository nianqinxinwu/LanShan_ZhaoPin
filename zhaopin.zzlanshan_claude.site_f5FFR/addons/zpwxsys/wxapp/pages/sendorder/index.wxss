.header_2{
  width:750rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  padding-top:30rpx;
  padding-bottom: 30rpx;
	background:#fff;
  
	}
	.header_2_fixed{

		width:750rpx;
		display: flex;
		flex-direction: row;
		justify-content: space-around;
		padding-top:30rpx;
		padding-bottom: 30rpx;
		background:#fff;
		position: fixed;
		z-index: 100;
		top:0px;

	}



	.navlist{

    width:750rpx;
    background-color:#fff;
    padding: 10rpx;
    

  }

  .lists{

    width:720rpx;
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    justify-content: flex-start;
  }
  
  .lists .item{
  
  
    width:110rpx;
    height:50rpx;
    background: #fff;
    border-radius:10rpx;
    margin-bottom: 20rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 11px;
    margin-right: 10rpx;

  
  }


  .city_2{
  height: 80rpx;
    line-height: 80rpx;
    color: #000000;
    font-size: .9rem;
    font-weight: 600;
  }
  .search_2{
  
    width:500rpx;
    height: 80rpx;
    line-height: 80rpx;
    background:#f0f1f5;
    display: flex;
    flex-direction: row;
    justify-content: center;
    border-radius: 30rpx 30rpx 30rpx 0rpx ;
  
  }
  .search_2 view{
      color:#a6a6a6;
      font-size: .7rem;
      align-self: center;
  
  }
  .search_2 icon{
  
  margin-right: 20rpx;
  margin-top: 18rpx;
  
	}
	


	.selectcars{

    width:750rpx;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    padding:20rpx;
    font-size:.8rem;
    background: #FFFFFF;
  
  }
  .selectcars_fixed{

    width:750rpx;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    padding:20rpx;
    font-size:.8rem;

    z-index: 1112;
    position: fixed;
    top: 120rpx;
    background: #fff;

  }
  
   .itemselectcars{
  
    display: flex;
    flex-direction: row;
    height: 60rpx;
    justify-content: center;
		align-items: center;
    width:33%;

    
		
  }
  
   .itemselectcars image{
  
    width:15rpx;
    height: 15rpx;
    align-self: center;
    margin-left:5rpx;

  }

  .red{
    color:#fd6a36;
  }



  .select_cars,
.select_sort,
.select_price {
	position: fixed;
  top: 92px;
  left:0px;
	z-index: 888;
  width: 750rpx;
  padding-left:30rpx;

	background-color: #fff;
}

.select_cars text,
.select_sort text {
	display: block;
	flex: 1;
	line-height: 35px;
	font-size: 13px;
}

.select_cars text:last-child,
.select_sort text:last-child {
	border-bottom: 0;
}

.select_cars_list {
	display: flex;
	border-bottom: 1px solid #ececec;
}

.select_cars_list image {
	width: 12px;
	height: 22px;
  padding-top: 8px;
  margin-right: 30rpx;
}
.select {
	border-bottom: 1px solid #e62129;
}

.select text {
	color: #e62129;
}

.select_price {
  display: flex;
  flex-direction: row;
  justify-content: center;
	flex-wrap: wrap;
  padding: 6px 4%;
  padding-top:30rpx;

}
.select_price .item{

 
  height: 80rpx;
  background: #f3f3f3;
  padding-left:15rpx;
  padding-right: 15rpx;
  border-radius: 35rpx;
  margin-right: 22rpx;
  margin-bottom: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size:12px;
}



/* 遮罩层 */
.small_overlay {
	position: fixed;
  width: 100%;
  height: 80%;
  bottom: 0;
  left: 0;
  z-index: 666;
  background-color: rgba(0,0,0,0.5);
}

.imglabel{

  position: absolute;
  top:-10rpx;
  left:-15rpx;

}

.news{

  width:750rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}


.guestlist{
  width:700rpx;
  display: flex;
  flex-direction: column;
  background-color: #fff;
  margin-top:20rpx;
  border-radius: 5rpx;
  padding:20rpx;
  
}
.guestlist .username{


  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.guestlist .username view:nth-child(1){

  font-size: 16px;
  font-weight: 600;
}

.guestlist .username view:nth-child(2){

  font-size: 12px;
}

.guestlist .usertel{
  font-size:14px;
  margin-top: 20rpx;
}

.guestlist .useraddress{
  font-size:14px;
  margin-top: 20rpx;
  width:650rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.guestlist .useraddress view:nth-child(2){

  font-weight: bold;
  font-size: 14px;
  color: #ff9003;
}
.guestlist .useroper{
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
}

.guestlist .useroper view{
  margin-top: 20rpx;
 
  padding:10rpx;
  background: #fff;
  border-radius:10rpx;
  margin-bottom: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 11px;
  border: 1rpx solid #ff9003;
  color:#ff9003;
  margin-left:15rpx;

}



 .newslist{
  display: flex;
  flex-direction: row;
  width:650rpx;
  margin: 0 auto;
  margin-bottom:20rpx;
  background: #fff;
  
  }
  
  
  
   .newslist .title{
  width:100%;
  flex-direction: column;
  display: flex;
  justify-content: space-between;
  
  }
  
  
   .newslist .title .detail{

    text-align: left;
    font-size: .8rem;
    color: #000; 
    display: flex;
    flex-direction: row;
    width:100%;
  }
  
   .newslist .title .detail .pic {
    width:20%;
    display: flex;
    flex-direction: column;
    align-items: center;
    position: relative;
  }
  
   .newslist .title .detail .pic image{
    width:120rpx;
    height: 120rpx;
    align-self: center;
    border-radius: 60rpx;
  }
   .newslist .title .detail .pic text{
  
    font-size: 10px;
    color: #646464; 
  }
  
  
  .footimg{
    width:40rpx;
    height:40rpx;
    position:absolute;
    left: 120rpx;  
    right: 0;
    top:80rpx;  
  
  }
  
  
   .newslist .title .detail .info{
  
      width:65%;
  }
  
   .newslist .title .detail .info  view:nth-child(1){
     padding-top: 10rpx;
     color:#000;
      font-weight: bold;
      display: flex;
      flex-direction: row;
      justify-content: space-between;
      font-size: 15px;
      
  }
  
   .newslist .title .detail .info  view:nth-child(2){
     padding-top: 10rpx;
     padding-bottom: 10rpx;
     color: #646464; 
        font-size: 12px;
  }
  
    .newslist .title .detail view:nth-child(3){
     padding-top: 10rpx;
  padding-right: 10rpx;
   }
   .newslist .title .price{
  
  width:28%;
  }
  
   .newslist .title .price .money{
  color:#d65408;
  font-weight:bold;
  font-size: .7rem;
  padding-right: 10rpx;
  }
  
  .jobname>text{
  color:#b7b7b7;
  
  }
  .jobmoney{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  color: #646464; 
  font-size: 13px;
  }
  
  .jobmoney text{
  
    color:#1296db;
    margin-left:10rpx;
  }
  .info .time{
  
    color:#ff4129;
    font-size: .7rem;
  }



.nobred{

  border: 1rpx solid #adaaa6;
  color:#adaaa6;
}

.bred{
    border: 1rpx solid #ff9003;
    color:#ff9003;

  }

  .nodata{

    width:750rpx;
    display: flex;
    align-items: center;
    justify-content: center;
  
    margin-top: 300rpx;
  
  }
  .nodata image{
  
    width:150rpx;
    height: 150rpx;
  }