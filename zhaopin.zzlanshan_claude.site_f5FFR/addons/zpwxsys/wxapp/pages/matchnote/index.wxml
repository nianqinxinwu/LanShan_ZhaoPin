
<view class="container_2">



  <view class="{{scrollTop>85 ? 'header_2_fixed' : 'header_2'}}" >

  

 按<text style="color:blue;">{{jobinfo.jobtitle}}</text>职位共匹配<text style="color:red;">{{notecount}}个</text>简历
</view>
 

 









































<!-- 列表 -->
<view class="news">
          <view class="newslist" wx:for="{{notelist}}">
                            <view class="title" bindtap="toWorkerDetail"  data-id="{{item.id}}">
                                     

                                       <view class="detail">
                                       
                                           <view class="info">
                                            <view>{{item.name}} </view>
                                            <view> {{item.sex == 1 ? '男' : '女'}} / {{item.age}}岁 / {{item.education}} / {{item.express}} </view>
                                            <view class="jobmoney">意向职位: <text> {{item.jobtitle}}</text></view>
                                            </view>

                                            <view class="pic">
                                           <image src="{{item.avatarUrl}}"></image>

                                        

                                           <text class="time">{{item.refreshtime}}</text>

                                           <image src="../../imgs/icon/{{item.sex == 1 ? 'man' : 'sex'}}.png" class="footimg" style="width:30rpx;height:30rpx;"></image>

                                           </view>

                                        

                                      </view>
                            </view>
                </view>



<view class="logincompany"  wx:if="{{companyid == 0}}">

<view>成为企业会员，高效挑选人才!</view>

<view><button bindtap="toLogin">登录企业</button></view>
<view bindtap="doCall" data-tel ="{{intro.tel}}">客服服务热线:{{intro.tel}}</view>

</view>






</view>


</view>



<!-- 加载更多 -->
<view class="loadmore">
	<text>{{loadMore}}</text>
</view>

<view class="u-tap-btn">

  <button class="u-go-home2" open-type='contact'>
<view class="iconfont fa fa-comments-o" ></view>
  </button>
</view>

