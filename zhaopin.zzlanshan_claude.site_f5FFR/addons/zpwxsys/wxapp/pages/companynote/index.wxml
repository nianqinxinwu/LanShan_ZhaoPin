<!-- 列表 -->
<view class="news" wx:if="{{notelist.length>0}}">

   

    <view class="guestlist" wx:for="{{notelist}}">


        <view class="newslist" >
                            <view class="title">
                                     

                                       <view class="detail">
                                       
                                           <view class="info">
                                            <view>求职者:{{item.name}} </view>
                                            <view> {{item.sex == 1 ? '男' : '女'}} / {{item.age}}岁 / {{item.education}} / {{item.express}} </view>
                                            <view class="jobmoney">意向职位: <text> {{item.jobtitle}}</text></view>
                                            </view>

                                            <view class="pic">
                                           <image src="{{item.avatarUrl}}"></image>

                                        

                                           <text class="time">{{item.refreshtime}}</text>

                                           <image src="../../imgs/icon/{{item.sex == 1 ? 'man' : 'sex'}}.png" class="footimg" style="width:30rpx;height:30rpx;"></image>

                                           </view>

                                        

                                      </view>
                            </view>
                </view>

                <view class="username"><view> </view><view>变更时间:{{item.createtime}}</view></view>

        <view class="usertel">向【{{item.companyname}}】应聘【{{item.jobtitle}}】</view>
        <view class="usertel" wx:if="{{item.agentuid>0}}"><view class="cu-capsule radius">
    <view class="cu-tag bg-red ">
        来源：
    </view>
    <view class="cu-tag line-red">
        经纪人推荐
    </view>
  </view></view>
        
        <view class="usertel"><view class="cu-capsule radius">
    <view class="cu-tag bg-red ">
        状态：
    </view>
    <view class="cu-tag line-red">
        {{item.status_str}}
    </view>
  </view></view>



        <view class="useraddress"><view>{{item.content}}</view><view hidden="true">佣金 {{item.money}}元</view></view>





        <view class="useroper">




         <view bindtap="toWorkerDetail" data-id="{{item.noteid}}">查看求职者</view>

        

         <view bindtap="toPay" data-id="{{item.id}}" >进度处理</view>


         </view>

        
    </view>







              </view>




<view class="nodata" wx:if="{{notelist.length == 0}}">
    <image   src="../../imgs/icon/nodata.png"></image>
</view>       


</view>



<!-- 加载更多 -->
<view class="loadmore">
	<text>{{loadMore}}</text>
</view>



