<!-- search -->



<view class="container_2">










<!-- 列表 -->
<view class="news"> 

   

    
    
    <view wx:if="{{list.length>0}}"  class="joblist_2" wx:for="{{list}}">
                              <view class="title_2" bindtap="toCompanyDetial" data-id="{{item.companyid}}">
  
                                <view class="jobinfo_2">

                                  <view class="logo">
                                    <image src="{{item.thumb}}"></image>
                                  </view>
  
                                      <view class="jobinfoleft_2">
                                                      <view class="address_2">
                                                              <view class="jobname_2">{{item.companyname}} </view>  
  
                                                              <view  class="moneyinfo_2">
                                                              
                                                              
                                                              </view>
                                                             
                                                      </view>
  
                                                
  
                                                      <view class="special_2">
                                                   
                                                    <view class="items_2" >{{item.companytype}}</view> 
                                                    <view class="items_2" >{{item.companyworker}}</view>
                                                    <view class="items_2" >{{item.companycate}}</view>
                                                      </view>
   
                                        </view>
  
                                   
  
  
                              
                              </view>
  
  
  
  
                                         <view class="detail_2">
                                            
                                             <view class="info_2">
                                            
                                                    <view class="companyinfo_2" style="font-size:.9rem;"> 
                                                  您于{{item.createtime}}关注了该企业
                                                       
                                                    </view>

                                              </view>
                                        </view>
                                        <view class="gz-actions">
                                          <view class="btn-ungz" catchtap="cancelGz" data-companyid="{{item.companyid}}">取消关注</view>
                                        </view>
                              </view>
                  </view>
  
  
     
  
  
                  <view class="nodata" wx:if="{{list.length == 0}}">
    <image   src="../../imgs/icon/nodata.png"></image>
</view>



              </view>


</view>



<!-- 加载更多 -->
<view class="loadmore">
	<text>{{loadMore}}</text>
</view>





