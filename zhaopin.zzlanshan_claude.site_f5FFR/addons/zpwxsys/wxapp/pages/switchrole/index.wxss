/* pages/login/index.wxss */
page{
    background-color: #fff;
}
.head{
    display: flex;
    flex-direction: column;
    padding: 47rpx 50rpx 50rpx;
}
.head>text{
    font-size: 26rpx;
    color: #999999;
}
.head>.tit{
    font-size: 50rpx;
    font-weight: bold;
    color: #333333;
    margin-bottom: 6rpx;
}
.user_list{
    display: flex;
    flex-direction: column;
    padding-left: 50rpx;
    padding-right: 50rpx;
}
.user_list>view{
    display: flex;
    margin-bottom: 32rpx;
}
.user_list>view>image{
    width: 650rpx;
    height: 200rpx;
    position: absolute;
    z-index: -1;
    
}
.user_list>view>view{
    display: flex;
    flex-direction: column;
    height: 200rpx;
    justify-content: center;
    margin-left: 370rpx;
}
.user_list>view>view text{
    font-size: 28rpx;
    color: #AEAEB9;
}
.user_list>view>view .use_tit{
    font-size: 36rpx;
    font-weight: bold;
    color: #333333;
    margin-bottom: 12rpx;
    
}

.choice{
    display: flex;
    align-content: center;
    justify-content: center;
}
.choice text{
    font-size: 28rpx;
    color: #999999;
    padding-top: 28rpx;
    padding-bottom: 28rpx;
}